<div class="container p-4">
    <div class="row">
        {{#each links}}
        <div class="col-md-3">
            <div class="card text-center">
                <div class="card-body">
                    <a href="{{url}}" target="_blank">
                        <h3 class="card-title text-uppercase">{{titulo}}</h3>
                    </a>
                    <p class="m-2">{{descripcion}}</p>
                    <p class="text-muted">{{timeago creado}}</p>
                    <a class="btn btn-danger" href="">Borrar</a>
                    <a class="btn btn-secondary" href="">Editar</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>